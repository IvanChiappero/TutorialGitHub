<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuAdministrador">

    <Button
            android:text="Crear Evento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="68dp"
            tools:layout_editor_absoluteX="130dp" android:id="@+id/btnCrearEvento"/>
    <Button
            android:text="Eliminar Evento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="160dp"
            tools:layout_editor_absoluteX="120dp" android:id="@+id/btnEliminarEvento"/>
    <Button
            android:text=" Eliminar Usuario"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="244dp"
            tools:layout_editor_absoluteX="119dp" android:id="@+id/btnEliminarUsuario"/>
    <Button
            android:text="Salir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="334dp"
            tools:layout_editor_absoluteX="148dp" android:id="@+id/btnSalir"/>
</android.support.constraint.ConstraintLayout>