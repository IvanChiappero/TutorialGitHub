<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CrearEvento">

    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="39dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView3"/>
    <TextView
            android:text="Fecha Inicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="97dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView8"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="16dp" tools:layout_editor_absoluteX="152dp"
            android:id="@+id/txtNombreEvento"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            tools:layout_editor_absoluteY="77dp" tools:layout_editor_absoluteX="152dp" android:id="@+id/txtFechInicio"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            tools:layout_editor_absoluteY="149dp" tools:layout_editor_absoluteX="152dp" android:id="@+id/txtFechaFin"/>
    <TextView
            android:text="Fecha Fin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="162dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView9"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="209dp" tools:layout_editor_absoluteX="153dp" android:id="@+id/txtLugar"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="270dp" tools:layout_editor_absoluteX="153dp" android:id="@+id/txtURL"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="334dp" tools:layout_editor_absoluteX="152dp"
            android:id="@+id/txtDescripcion"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            tools:layout_editor_absoluteY="389dp" tools:layout_editor_absoluteX="153dp" android:id="@+id/txtPrecio"/>
    <TextView
            android:text="URL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="285dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView10"/>
    <TextView
            android:text="Lugar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="222dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView11"/>
    <TextView
            android:text="Descripcion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="349dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView12"/>
    <TextView
            android:text="Precio"
            android:layout_width="wrap_content"
            android:layout_height="23dp" tools:layout_editor_absoluteY="403dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView13"/>
    <Button
            android:text="Agregar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="446dp"
            tools:layout_editor_absoluteX="63dp" android:id="@+id/btnAgregar" android:onClick="crearEvento"/>
    <Button
            android:text="Salir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="446dp"
            tools:layout_editor_absoluteX="226dp" android:id="@+id/btnSalir"/>
</android.support.constraint.ConstraintLayout>